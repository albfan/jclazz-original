package ru.andrew.jclazz.apps.tester.tests.blocks;

public class TryWithBreakContinue
{

    public TryWithBreakContinue()
    {
    }
    
    public void test1()
    {
        int int_1 = 1;
        do
        {
            System.out.println("START DO");
            try
            {
                System.out.println("TRY");
            }
            catch (Exception exception_1)
            {
                System.out.println("CATCH");
                break;
            }
            System.out.println("END LOOP");
        }
        while (0 != int_1);
    }
    
    public void test2()
    {
        int int_1 = 1;
        do
        {
            System.out.println("START DO");
            try
            {
                System.out.println("TRY");
                break;
            }
            catch (Exception exception_1)
            {
                System.out.println("CATCH");
                System.out.println("END LOOP");
            }
        }
        while (0 != int_1);
    }
    
    public void test3()
    {
        int int_1 = 1;
        do
        {
            System.out.println("START DO");
            try
            {
                System.out.println("TRY");
            }
            catch (Exception exception_1)
            {
                System.out.println("CATCH");
                continue;
            }
            System.out.println("END LOOP");
        }
        while (0 != int_1);
    }
    
    public void test4()
    {
        int int_1 = 1;
        do
        {
            System.out.println("START DO");
            try
            {
                System.out.println("TRY");
            }
            catch (Exception exception_1)
            {
                System.out.println("CATCH");
                System.out.println("END LOOP");
            }
        }
        while (0 != int_1);
    }
    
}
