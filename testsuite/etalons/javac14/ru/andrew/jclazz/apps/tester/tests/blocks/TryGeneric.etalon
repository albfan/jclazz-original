    public void test5(String string_1)
    {
        try
        {
            if ("".equals(string_1))
            {
                throw new RuntimeException();
            }
            System.out.println("SOUT");
        }
        catch (Exception exception_1)
        {
            System.out.println("EXCEPTION");
        }
        finally
        {
            System.out.println("FINALLY");
        }
    }
    
    public void test6(String string_1) throws Throwable
    {
        try
        {
            if ("".equals(string_1))
            {
                throw new Throwable();
            }
            System.out.println("SOUT");
        }
        catch (Exception exception_1)
        {
            System.out.println("EXCEPTION");
        }
        finally
        {
            System.out.println("FINALLY");
        }
    }
    
