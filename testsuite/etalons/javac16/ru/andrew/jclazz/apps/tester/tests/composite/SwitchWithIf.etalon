package ru.andrew.jclazz.apps.tester.tests.composite;

public class SwitchWithIf
{

    public SwitchWithIf()
    {
    }
    
    public void test1(int int_1, int int_2)
    {
        switch (int_1)
        {
            case 31:
                if (int_2 == 7)
                {
                    System.out.println("IF");
                }
                else
                {
                    System.out.println("ELSE");
                }
        }
    }
    
}
