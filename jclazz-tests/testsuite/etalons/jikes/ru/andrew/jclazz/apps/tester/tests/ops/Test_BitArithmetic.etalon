package ru.andrew.jclazz.apps.tester.tests.ops;

public class Test_BitArithmetic
{

    public void test1()
    {
        int int_1 = 1;
        long long_1 = 1L;
        int int_2 = int_1 << 4;
        long long_2 = long_1 << 4;
        int int_3 = int_1 >> 4;
        long long_3 = long_1 >> 4;
        int int_4 = int_1 >>> 4;
        long long_4 = long_1 >>> 4;
        int int_5 = int_1 & 4;
        long long_5 = long_1 & (long) 4;
        int int_6 = int_1 | 4;
        long long_6 = long_1 | (long) 4;
        int int_7 = int_1 ^ 4;
        long long_7 = long_1 ^ (long) 4;
    }
    
    public void test2()
    {
        int int_1 = 3;
        int int_2 = 1;
        long long_1 = 1L;
        int int_3 = int_2 << int_1;
        long long_2 = long_1 << int_1;
        int int_4 = int_2 >> int_1;
        long long_3 = long_1 >> int_1;
        int int_5 = int_2 >>> int_1;
        long long_4 = long_1 >>> int_1;
        int int_6 = int_2 & int_1;
        long long_5 = long_1 & (long) int_1;
        int int_7 = int_2 | int_1;
        long long_6 = long_1 | (long) int_1;
        int int_8 = int_2 ^ int_1;
        long long_7 = long_1 ^ (long) int_1;
    }
    
    public Test_BitArithmetic()
    {
    }
    
}
