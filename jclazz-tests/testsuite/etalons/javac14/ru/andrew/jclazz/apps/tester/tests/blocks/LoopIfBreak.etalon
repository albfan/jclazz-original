public class LoopIfBreak
{

    public LoopIfBreak()
    {
    }
    
    public void test1()
    {
        int int_1 = 0;
        while (int_1 < 10)
        {
            System.out.println("START LOOP");
            if (int_1 == 5)
            {
                System.out.println("IF");
                break;
            }
            int_1++;
            System.out.println("END LOOP");
        }
    }
    
