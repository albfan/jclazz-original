package ru.andrew.jclazz.apps.tester.tests.ops;

public class Test_Arithmetic
{

    public Test_Arithmetic()
    {
    }

    public void test()
    {
        int int_1 = 1;
        int int_2 = 2;
        long long_1 = 1L;
        long long_2 = 2L;
        float float_1 = 1.0f;
        float float_2 = 2.0f;
        double double_1 = 1.0;
        double double_2 = 2.0;
        System.out.println("Add");
        int int_3 = int_1 + int_2;
        long long_3 = long_1 + long_2;
        float float_3 = float_1 + float_2;
        double double_3 = double_1 + double_2;
        System.out.println("Sub");
        int int_4 = int_1 - int_2;
        long long_4 = long_1 - long_2;
        float float_4 = float_1 - float_2;
        double double_4 = double_1 - double_2;
        System.out.println("Mul");
        int int_5 = int_1 * int_2;
        long long_5 = long_1 * long_2;
        float float_5 = float_1 * float_2;
        double double_5 = double_1 * double_2;
        System.out.println("Div");
        int int_6 = int_1 / int_2;
        long long_6 = long_1 / long_2;
        float float_6 = float_1 / float_2;
        double double_6 = double_1 / double_2;
        System.out.println("Rem");
        int int_7 = int_1 % int_2;
        long long_7 = long_1 % long_2;
        float float_7 = float_1 % float_2;
        double double_7 = double_1 % double_2;
        System.out.println("Neg");
        int int_8 = -int_1;
        long long_8 = -long_1;
        float float_8 = -float_1;
        double double_8 = -double_1;
    }

}
