package ru.andrew.jclazz.apps.tester.tests.ops;

public class Test_Switch
{

    public Test_Switch()
    {
    }

    public void testSwitch1(int int_1)
    {
        System.out.println("BEFORE SWITCH");
        switch (int_1)
        {
            case 0:
                System.out.println("0");
                break;
            case 10:
                System.out.println("10");
                break;
            case 50:
                System.out.println("50");
                return;
            default:
                System.out.println("DEFAULT");
        }
        System.out.println("AFTER SWITCH");
    }

    public void testSwitch2(int int_1)
    {
        System.out.println("BEFORE SWITCH");
        switch (int_1)
        {
            case 0:
                System.out.println("0");
            case 1:
                System.out.println("1");
                break;
            case 2:
                System.out.println("2");
                return;
            default:
                System.out.println("DEFAULT");
        }
        System.out.println("AFTER SWITCH");
    }

    public void testSwitch3(int int_1)
    {
        System.out.println("BEFORE SWITCH");
        switch (int_1)
        {
            case 0:
                System.out.println("0");
        }
        System.out.println("AFTER SWITCH");
    }

    public void testSwitch4(int int_1)
    {
        System.out.println("BEFORE SWITCH");
        switch (int_1)
        {
            case 0:
            case 1:
                System.out.println("1");
                break;
            case 2:
                System.out.println("2");
                return;
            default:
        }
        System.out.println("AFTER SWITCH");
    }

}
